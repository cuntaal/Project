package factorial;

import java.util.Scanner;

public class App {
  public static void main (String[]arg){
	  int num;
	  int cal;
	  int n=0;
	  
	  System.out.println("Enter Number Here:");
      Scanner input = new Scanner(System.in);
      num = input.nextInt();
      
      for ( cal = 1 ; cal <= num ; cal++ )
          n = n*cal;

       System.out.println("Factorial is = "+n);
  }
}
